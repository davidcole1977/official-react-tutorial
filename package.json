{
  "name": "official-react-tutorial",
  "engines": {
    "node": "0.12.x"
  },
  "dependencies": {
    "body-parser": "~1.4.3",
    "express": "~4.4.5",
    "jquery": "~2.2.3",
    "marked": "~0.3.5",
    "react": "~15.0.0",
    "react-dom": "~15.0.0"
  },
  "devDependencies": {
    "babel-eslint": "~6.0.2",
    "babel-loader": "~6.2.4",
    "babel-preset-es2015": "~6.5.0",
    "babel-preset-react": "~6.5.0",
    "babel-register": "~6.5.2",
    "chai": "~3.5.0",
    "chai-equal-jsx": "~1.0.9",
    "enzyme": "~2.2.0",
    "eslint": "~2.7.0",
    "eslint-plugin-react": "~4.3.0",
    "mocha": "~2.4.5",
    "react-addons-test-utils": "~15.0.0",
    "sinon": "~1.17.3",
    "webpack": "~1.12.15"
